{
  "name": "vue3-app-starter",
  "version": "1.0.4",
  "description": "The application template for Vue.js 3.x",
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "eslint": "^8.56.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "rollup-plugin-license": "^3.2.0",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-scss": "^6.1.0",
    "vite": "^5.0.12"
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "webpack --config webpack.dev.js --watch",
    "start": "vite dev",
    "lint:eslint": "eslint src/**/*.{js,vue}",
    "lint:stylelint": "stylelint src/**/*.{css,scss,vue}",
    "lint:prettier": "prettier src/**/*.{js,css,scss,vue} --check",
    "lint": "run-p lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-s fix:*"
  },
  "private": true,
  "keywords": [],
  "author": "tmiyachi",
  "license": "MIT"
}
